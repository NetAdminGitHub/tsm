

<form id="FrmNotaRemisionCliente" method="POST" enctype="multipart/form-data" autocomplete="off">
    <div class="col-lg-12">
        <div class="form-row">
            <div class="col-lg-4 form-group">
                <label for="txtSerie">Serie:</label>
                <input id="txtSerie" name="txtSerie" type="text" class="k-textbox form-control input-number" maxlength="20" />
            </div>
            <div class="col-lg-4 form-group">
                <label for="txtNoDocumento">Número:</label>
                <input id="txtNoDocumento" name="txtNoDocumento" type="text" class="k-textbox form-control input-number" maxlength="20" />
            </div>
            <div class="col-lg-4 form-group">
                <label for="FechaDocumento">Fecha:</label>
                <input id="FechaDocumento" name="FechaDocumento" type="text" class="form-control" />
            </div>
            <div class="form-group col-lg-8">
                <label for="txtDescripcion">Descripción:</label>
                <input id="txtDescripcion" name="txtDescripcion" class="form-control k-text" maxlength="250">
            </div>
            <div class="col-lg-4 form-group" style="text-align:end;align-self:center;">
                <button type="button" id="btnCrea_registroNota">Guardar Registro</button>
            </div>
        </div>
        <div class="form-row">
            <div class="col-lg-12 form-group">
                <div id="gridItemsDmSinNota" class="form-control"></div>
            </div>
         </div>
    </div>
</form>