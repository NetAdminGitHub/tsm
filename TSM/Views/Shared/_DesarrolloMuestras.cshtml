<div class="form-row">
    <div class="form-group col-lg-12">
        <div class="form-row">
            <div class="form-group col-lg-3">
                <label for="CmbMaquinaMues">Selección de Maquina:</label>
                <input id="CmbMaquinaMues" name="CmbMaquinaMues" required validationMessage="Requerido" type="text" class="form-control" />
            </div>
            <div class="col-lg-3"></div>
        </div>
        <div class="form-row">
            <div class="form-group col-lg-12 align-self-lg-end">
                <button type="button" id="btnMuest" data-role="button" class="k-button k-button-icontext" role="button" aria-disabled="false" tabindex="0"><span class="k-icon k-i-hyperlink-open-sm"></span>&nbsp;Limpiar Seteo</button>
                <button type="button" id="btnFinOT">Finalizar OT</button>
                <button type="button" id="btnConsultarPesos">Detalle de pesos</button>
