                    <span class="k-icon k-i-arrows-kpi"></span>&nbsp;Desplazar/Intercambiar
                </button>
                <button type="button" id="btnDuplicar_Mues" data-role="button" class="k-button k-button-icontext" role="button" aria-disabled="false" tabindex="0">
                    <span class="k-icon k-i-copy"></span>&nbsp;Duplicar Estación
                </button>

                <button type="button" id="btnAjuste_Mues" data-role="button" class="k-button k-button-icontext" role="button" aria-disabled="false" tabindex="0">
                    <span class="k-icon k-i-warning"></span>&nbsp;Ingresar ajuste tinta/marco
                </button>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-lg-3">
                <div id="dgAccesorios_Muest"></div>
            </div>
        </div>
