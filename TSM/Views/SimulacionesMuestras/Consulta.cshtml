@{
    ViewBag.Title = "Consulta de Simulaciones de Muestras";
    ViewBag.IdVersion = "TSM0101151|v22110101";
}
@section Scripts {
    <script src="~/Scripts/js/SimulacionesMuestras/Consulta.js?@ViewBag.IdVersion"></script>
}
<form id="frmBuscar" class="form-row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="form-row">
                    <div class="form-group col-lg-3">
                        <label for="CmbIdCliente">Cliente :</label>
                        <input id="CmbIdCliente" name="CmbIdCliente" class="form-control" />
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="CmbNoOrdenTrabajo">No. Orden de Trabajo</label>
                        <input id="CmbNoOrdenTrabajo" name="CmbNoOrdenTrabajo" type="text" class="form-control" />
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="CmbPrograma">No. Programa</label>
                        <input id="CmbPrograma" name="CmbPrograma" type="text" class="form-control" />
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="cmbNoFM">No. FM</label>
                        <input id="cmbNoFM" name="cmbNoFM" type="text" class="form-control" />
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-lg-12">
                        <div id="grid"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>