                            <div class="form-group col-lg-4">
                                <label for="CmbConsUnidad_Ajuste">Unidad de Medidad</label>
                                <input id="CmbConsUnidad_Ajuste" name="CmbConsUnidad_Ajuste" type="text" class="form-control" />
