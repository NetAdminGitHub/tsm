
@model Microsoft.PowerBI.Api.V2.Models.Report

@{
    ViewBag.Title = "Declaraciones de mercancías con existencias.";
    ViewBag.IdVersion = "TSM0102002|v20020301";
}

<script type="text/javascript" hidden="hidden">
    var PageN = "@TSM.Utils.PbiUtils.PbiReport.Pagina"; // nombre de la página.
    var ReportN = null;
    var WSpace = null;
    var EmbedUrl ="@TSM.Utils.PbiUtils.PbiEmbed";
    let Client = "@TSM.Utils.PbiUtils.PbiReport.Reporte";
    let Token = "";
    let navbarEnabled = "@TSM.Utils.PbiUtils.PbiReport.HabilitaBarraNav";

    try {
        Token =  "@TSM.Utils.PbiUtils.authResult.AccessToken";
    } catch (e) {
        Token = "";
    }
   

</script>

@section Scripts {
    <script type="text/javascript" src="~/Scripts/Utils/powerbi.js"></script>
    <script type="text/javascript" src="~/Scripts/js/pbiReporte.js"></script>
    
}



<div id="reportWrapper" class="form-row" >
    <div class="col-lg-12" >
        <div class="card" style="height:100%;">
            <div class="card-body">
                <div class="form-group" style="float:right;">
                    <button type="button" id="FullSize" onclick="_Report_FullScreen();" class="k-button k-button-icontext" style="width:140px; height:35px;"><span class="k-button-icon k-icon k-i-toggle-full-screen-mode"></span><p>Pantala Completa</p></button>
                </div>
            </div>
            <div class="card-body" style="height:100%;">
                 <div id="reportContainer" class="form-group" style="height:100%; ">
                    <iframe class="ReportFrame" frameborder="0" style="height:100%; " allowFullScreen="true" id="reportContainer" frameborder="0"></iframe>

                </div>

            </div>
        </div>
    </div>
</div>



