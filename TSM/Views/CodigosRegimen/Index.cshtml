
@{
    ViewBag.Title = "Códigos de Régimen de Aduanas";
    ViewBag.IdVersion = "TSM0101146|v22093001";
}

@section Scripts{
    <script src="~/Scripts/js/CodigosRegimen.js"></script>
}

<div class="form-row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body" style="height: 800px;">

                <div id="vertical" style="height: 720px;">

                    <div id="top-pane">

                        <div id="horizontal" style="height: 100%; width: 100%;">
                            <div id="left-pane">
                                <div class="pane-content">
                                    <div class="form-row">
                                        <div class="form-group col-lg-12">
                                            <div class="form-row">
                                                <div class="form-group col-lg-12">
                                                    <label for="gridRTTR">Relación Traslado Régimen:</label>
                                                    <div id="gridRTTR" class="form-control"></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="right-pane">
                                <div class="pane-content">
                                    <div class="row">

                                        <div class="form-group col-lg-12">
                                            <label for="gridTipoTraslado">Tipos de Traslado:</label>
                                            <div id="gridTipoTraslado" class="form-control"></div>
                                        </div>
                                        <div class="form-group col-lg-12">
                                            <label for="gridReg">Regímenes:</label>
                                            <div id="gridReg" class="form-control"></div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>

                </div>

            </div>
        </div>
    </div>
</div>