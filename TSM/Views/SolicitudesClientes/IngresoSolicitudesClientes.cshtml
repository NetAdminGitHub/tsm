@{
    ViewBag.Title = "Ingreso de solicitud del Cliente";
    ViewBag.IdVersion = "TSM0101060|v19092001";

}

@section Scripts{
    <script type="text/javascript">
        var vIdServSol = @ViewData["idServicio"];
        var esContacto =@ViewData["esContacto"];
        var idSolicitud=@ViewData["idSolicitud"];
    </script>
    <script src="~/Scripts/js/IngresoSolicitudesClientes.js"></script>
}

<div class="form-row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="form-group col-lg-12">
                    <form id="FrmClieCont" method="POST" enctype="multipart/form-data" autocomplete="off">
                        <div class="form-row">
                            <div class="form-group col-md-6 col-lg-3">
                                <label for="NoDocumento" hidden="hidden">No Solicitud:</label>
                                <input id="NoDocumento" name="NoDocumento" class="k-textbox form-control" , disabled="disabled" hidden="hidden" />
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-lg-10">
                                <label for="CmbCliCont">Cliente</label>
                                <input name="CmbCliCont" id="CmbCliCont" required validationMessage="Requerido" class="form-control">
                            </div>
                            <div class="form-group col-lg-2">
                                <label for="Fecha">Fecha</label>
                                <input id="Fecha" name="Fecha" required validationMessage="Requerido" class="form-control" />
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-lg-12">
                                <label for="CmbContactoSol">Contacto</label>
                                <input name="CmbContactoSol" id="CmbContactoSol" class="form-control">
                            </div>
                        </div>
                        <div class="form-row" id="divEje">
                            <div class="form-group col-lg-12">
                                <label for="CmbEjecSol">Ejecutivo(a) de cuentas</label>
                                <input name="CmbEjecSol" id="CmbEjecSol" class="form-control">
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-lg-9 align-self-lg-end">
                                <button type="button" id="btnAClieCont" hidden="hidden"></button>
                                <button type="button" id="btnDelClieCont"></button>
                                <button type="button" id="btnFinClieCont"></button>
                            </div>
                            <div class="form-group col-lg-3 align-self-lg-end">
                                <button type="button" id="btnProClieCont" hidden="hidden"></button>
                            </div>
                        </div>
                    </form>
                    <div class="form-row">
                        <div class="form-group col-lg-12">
                            <div id="gridDet"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="ModalMsgSol" class="modal fade" role="dialog">
    <div class="modal-dialog  modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title text-center" id="ModalClieEjeLabel"></h3>
                <button type="button" class="close" data-dismiss="modal" id="btnCloseMsg" aria-hidden="true">&times;</button>
            </div>
            <div class="modal-body">
                <div id="MshSol">
                    <div class="form-row">
                        <div class="form-group col-lg-4 text-center">
                            <i class="k-icon k-i-check" style="font-size:120px;"></i>
                        </div>
                        <div class="form-group col-lg-8">
                            <h2></h2>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>

<form id="FrmModalCliePro" method="POST" enctype="multipart/form-data" autocomplete="off">
    <div id="ModalCliePro" class="modal fade" role="dialog">
        <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h3 class="modal-title text-center" id="ModalClieProLabel"></h3>
                    <button type="button" class="close" data-dismiss="modal" id="btnClosePro" aria-hidden="true">&times;</button>
                </div>
                <div class="modal-body">
                    <div class="form-group col-lg-12">
                        <div class="form-row">
                            <div class="form-group col-lg-12">
                                <label for="CmbTempo">Temporadas</label>
                                <input name="CmbTempo" id="CmbTempo" required validationMessage="Requerido" class="form-control">
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-lg-12">
                                <label for="TxtPro">Programa</label>
                                <input name="TxtPro" id="TxtPro" required validationMessage="Requerido" type="text" class="k-textbox form-control">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer ">
                    <button type="button" id="btnACliePro">Crear</button>
                </div>
            </div>
        </div>
    </div>
</form>