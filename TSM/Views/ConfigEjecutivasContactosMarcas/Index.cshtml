@{
    ViewBag.Title = "Parametros de Comercialización Ejecutivas, Contactos y Marcas";
    ViewBag.IdVersion = "TSM0101076|v19102901";
}
@section Scripts {
    <script src="~/Scripts/js/ConfigEjecutivasContactosMarcas.js"></script>
}
<div class="form-row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
               

                <ul id="bpecm">
                    <li class="k-active form-control" >
                        <span class="TS-icon-USUARIOS" style="font-size: 1.7em;">&nbsp</span>
                        Configurar contactos y marcas
                        <div class="form-row">
                            <div class="form-group col-lg-7">
                                <label for="CmbCliente">Cliente</label>
                                <input id="CmbCliente" name="CmbCliente" type="text" class="form-control" />
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col-lg-12">
                                <div class="form-row">
                                    <div class="form-group col-lg-6">
                                        <label for="gridContacto">Asignación de Contactos:</label>
                                        <div id="gridContacto" class="form-control"></div>
                                    </div>
                                    <div class="form-group col-lg-6">
                                        <label for="gridMarca">Asignación de Marcas:</label>
                                        <div id="gridMarca" class="form-control"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </li>
                    <li  class="form-control" >
                        <span class="TS-icon-USUARIO" style="font-size: 1.7em;">&nbsp</span>
                        Configurar Ejecutiva de Cuenta
                        <div class="form-row">
                            <div class="form-group col-lg-7">
                                <label for="CmbEjecu">Ejecutivo (a)</label>
                                <input id="CmbEjecu" name="CmbEjecu" type="text" class="form-control" />
                            </div>
                            <div class="form-group col-lg-12">
                                <label for="CmbEjecu">Asignar Contactos</label>
                                <div id="gridContEjec" class="form-control"></div>
                            </div>
                        </div>
                    </li>
                </ul>


            </div>
        </div>
    </div>
</div>

