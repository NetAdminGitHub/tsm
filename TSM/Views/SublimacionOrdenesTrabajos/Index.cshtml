@{
    ViewBag.Title = "Sublimacion Ordenes de Trabajo";
    ViewBag.IdVersion = "TMS0101084|v19111801";
}
@section Scripts {
    <script src="~/Scripts/js/SublimacionOrdenesTrabajos.js"></script>
}
<div class="form-row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="form-row">
                    <div class="form-group col-lg-3">
                        <label for="CmbIdCliente">Cliente :</label>
                        <input id="CmbIdCliente" name="CmbIdCliente" class="form-control" />
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="CmbPrograma">Programa:</label>
                        <input id="CmbPrograma" name="CmbPrograma" type="text" class="form-control" />
                    </div>
                    <div class="form-group col-lg-3">
                        <label for="CmbOrdenTrabajo">Orden de Trabajo/No Requerimiento:</label>
                        <input id="CmbOrdenTrabajo" name="CmbOrdenTrabajo" type="text" class="form-control" />
                    </div>
                    <div class="form-group col-lg-3 align-self-end">
                        <div class="input-group">
                            <div style="width: 100%;">
                                <button type="button" id="btnNuevoRegistro" name="btnNuevoRegistro">Nuevo Requerimiento</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-lg-12">
                        <div id="gridConsulta"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


