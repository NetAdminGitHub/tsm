@{
    ViewBag.Title = "Consulta de Ingresos";
    ViewBag.IdVersion = "TSM0101111|v22053101";
}
@section Scripts {

    <script src="~/Scripts/js/ControlIngresos.js?@ViewBag.IdVersion"></script>
}
<style>
    tsm-label-center {
        display: block;
        text-align: center;
    }
</style>
<div class="form-row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="form-row">
                    <div class="col-lg-4">
                        <label for="cmbCliente" id="lblCliente">Cliente:</label>
                        <input id="cmbCliente" name="cmbCliente" class="form-control" /> @*required validationMessage="Requerido" class="form-control"*@
                    </div>
                    <div class="col-lg-4">
                        <label for="cmbPlanta" id="lblPlanta">Planta:</label>
                        <input id="cmbPlanta" name="cmbPlanta" class="form-control" /> @*required validationMessage="Requerido" class="form-control"*@
                    </div>
                    <div class="col-lg-2" style="text-align: center; align-self: end;">
                        <button type="button" id="btnIngresarMercancia">Ingresar Mercancía</button>
                    </div>
                    <div class="col-lg-2" style="text-align: center; align-self: end;" hidden="hidden">
                        <button type="button" id="btnImportarExcel">Importar Excel</button>
                    </div>
                </div>
                <div class="form-row"><br /></div>
                <div class="form-row">
                    <div class="col-lg-12">
                        <div id="gridIngreso"></div>
                    </div>
                </div>

                @*<div class="form-group">
                        <div id="grid"></div>
                    </div>*@
            </div>
        </div>
    </div>
</div>
@*ingreso control de bultos*@
<div id="vMod_controlbulto"></div>

@*ingreso Crear lista empaque *@
<div id="vMod_CrearListaEmpaque"></div>